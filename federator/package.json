{
  "name": "federator",
  "private": "true",
  "version": "2.0.0",
  "description": "RSK Bridge Federator",
  "keywords": [
    "rsk",
    "federated",
    "bridge",
    "ethereum",
    "federator"
  ],
  "author": "IOVLabs",
  "scripts": {
    "start": "node ./built/federator/src/main.js",
    "start-js": "node ./src/main.js",
    "build-start": "npm run build && npm run start",
    "build": "npm run clean && tsc",
    "test": "jest --detectOpenHandles test/",
    "integrationTest": "rm -rf ./db/* && node ./integrationTest/integrationTest.js",
    "integrationHeartbeatTest": "rm -rf ./db/* && node ./integrationTest/integrationHeartbeatTest.js && rm heartbeat.log",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "lint": "eslint . --ext .ts",
    "clean": "rm -rf ./db/* ./built"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "ethereumjs-tx": "^1.3.7",
    "ethereumjs-util": "^6.1.0",
    "express": "^4.17.1",
    "log4js": "^5.0.0",
    "web3": "^1.3.6"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.1",
    "jest": "^26.4.2",
    "node-notifier": "^8.0.1",
    "prettier": "2.3.2",
    "web3-core-promievent": "^1.2.11"
  },
  "engines": {
    "node": ">=16"
  }
}
